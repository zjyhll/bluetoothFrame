<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground"
    android:orientation="vertical" >

    <com.hwacreate.outdoor.view.ReboundScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white" >

                <TextView
                    android:id="@+id/tv_wifi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:padding="10dp"
                    android:text="@string/tv_wifi"
                    android:textSize="@dimen/size14" />

                <ToggleButton
                    android:id="@+id/tb_wifi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_horizontal"
                    android:background="@android:color/transparent"
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white" >

                <TextView
                    android:id="@+id/tv_phonelocation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:padding="10dp"
                    android:text="@string/tv_phonelocation"
                    android:textSize="@dimen/size14" />

                <ToggleButton
                    android:id="@+id/tb_phonelocation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_horizontal"
                    android:background="@android:color/transparent"
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white" >

                <TextView
                    android:id="@+id/tv_outh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:padding="10dp"
                    android:text="@string/tv_outh"
                    android:textSize="@dimen/size14" />

                <ToggleButton
                    android:id="@+id/tb_outh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_horizontal"
                    android:background="@android:color/transparent"
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_clear_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_clear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_clear"
                    android:textSize="@dimen/size14" />

                <TextView
                    android:id="@+id/tv_m"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:drawableRight="@drawable/right_s"
                    android:gravity="center"
                    android:text="0KB"
                    android:textColor="@color/app_green"
                    android:textSize="@dimen/size12" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_dituclear_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_dituclear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="离线地图管理"
                    android:textSize="@dimen/size14" />
                 <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:drawableRight="@drawable/right_s"
                    android:gravity="center"
                    android:textColor="@color/app_green"
                    android:textSize="@dimen/size12" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginTop="20dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_yijian_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_yijian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_yijian"
                    android:textSize="@dimen/size14" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/right_s"
                    android:contentDescription="@string/app_name" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_gengxin_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_gengxin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_gengxin"
                    android:textSize="@dimen/size14" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:drawableRight="@drawable/right_s"
                    android:gravity="center"
                    android:textColor="@color/content" />

                <View
                    android:id="@+id/tv_gengxin_red"
                    android:layout_width="8dp"
                    android:layout_height="8dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="8dp"
                    android:layout_toRightOf="@+id/tv_gengxin"
                    android:background="@drawable/shape_cicle_soild"
                    android:visibility="gone" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_help_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_help"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_help"
                    android:textSize="@dimen/size14" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/right_s"
                    android:contentDescription="@string/app_name" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_guanyu_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_guanyu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_guanyu"
                    android:textSize="@dimen/size14" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/right_s"
                    android:contentDescription="@string/app_name" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_guanyushebei_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_guanyushebei"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_guanyushebei"
                    android:textSize="@dimen/size14" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/right_s"
                    android:contentDescription="@string/app_name" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <RelativeLayout
                android:id="@+id/tv_xiugaimima_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_item_down"
                android:padding="10dp" >

                <TextView
                    android:id="@+id/tv_xiugaimima"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/tv_xiugaimima"
                    android:textSize="@dimen/size14" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/right_s"
                    android:contentDescription="@string/app_name" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/videoview" />

            <TextView
                android:id="@+id/tv_tuichudenglu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:background="@drawable/select_vip_goumai"
                android:gravity="center"
                android:padding="10dp"
                android:text="退出登录"
                android:textColor="@color/white"
                android:textSize="@dimen/size16" />
        </LinearLayout>
    </com.hwacreate.outdoor.view.ReboundScrollView>

</LinearLayout>