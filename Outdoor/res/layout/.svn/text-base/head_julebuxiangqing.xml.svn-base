<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <com.hwacreate.outdoor.view.MyViewPager
            android:id="@+id/julebu_mvp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:overScrollMode="never" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/indicator"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:gravity="center"
                android:orientation="horizontal" >
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/julebu_icon"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_weight="0"
                android:scaleType="fitXY" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="8dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/julebu_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/title"
                    android:textSize="@dimen/size16"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/julebu_dizhi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/title"
                    android:textSize="@dimen/size15" />

                <TextView
                    android:id="@+id/julebu_chuang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/title"
                    android:textSize="@dimen/size15" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text="等级:"
                    android:textColor="@color/content"
                    android:textSize="@dimen/size15" />

                <TextView
                    android:id="@+id/julebu_dengji"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/title"
                    android:textSize="@dimen/size15" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text="成员:"
                    android:textColor="@color/content"
                    android:textSize="@dimen/size15" />

                <TextView
                    android:id="@+id/julebu_chengyuan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/title"
                    android:textSize="@dimen/size15" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text="活动:"
                    android:textColor="@color/content"
                    android:textSize="@dimen/size15" />

                <TextView
                    android:id="@+id/julebu_huodong"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="@color/title"
                    android:textSize="@dimen/size15" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:singleLine="true"
                android:text="联系电话:"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />

            <TextView
                android:id="@+id/julebu_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/title"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:singleLine="true"
                android:text="QQ:"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />

            <TextView
                android:id="@+id/julebu_qq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/title"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:singleLine="true"
                android:text="俱乐部邮箱:"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />

            <TextView
                android:id="@+id/julebu_mail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/title"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:singleLine="true"
            android:text="俱乐部介绍"
            android:textColor="@color/content"
            android:textSize="@dimen/size15" />

        <TextView
            android:id="@+id/julebu_des"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:text=""
            android:textColor="@color/title"
            android:textSize="@dimen/size15" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/videoview" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/julebu_huodong_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/select_item_down"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:gravity="center"
                android:paddingLeft="6dp"
                android:paddingRight="6dp"
                android:text="活动"
                android:textSize="@dimen/size16" />

            <View
                android:id="@+id/julebu_huodong_view"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/app_green" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/julebu_julebu_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/select_item_down"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:gravity="center"
                android:paddingLeft="6dp"
                android:paddingRight="6dp"
                android:text="游记"
                android:textSize="@dimen/size16" />

            <View
                android:id="@+id/julebu_julebu_view"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/app_green"
                android:visibility="gone" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/julebu_lingdui_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/select_item_down"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:gravity="center"
                android:paddingLeft="6dp"
                android:paddingRight="6dp"
                android:text="领队"
                android:textSize="@dimen/size16" />

            <View
                android:id="@+id/julebu_lingdui_view"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/app_green"
                android:visibility="gone" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/julebu_chengyuan_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/select_item_down"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:gravity="center"
                android:paddingLeft="6dp"
                android:paddingRight="6dp"
                android:text="队员"
                android:textSize="@dimen/size16" />

            <View
                android:id="@+id/julebu_chengyuan_view"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/app_green"
                android:visibility="gone" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>