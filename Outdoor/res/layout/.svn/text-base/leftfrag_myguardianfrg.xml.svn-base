<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/pull_refresh_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground"
    android:orientation="vertical" >

    <include
        android:id="@+id/ll_title"
        layout="@layout/head_otheract" />

    <com.hwacreate.outdoor.view.ReboundScrollView
        android:id="@+id/rsv_youji"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/ll_bottom"
        android:layout_below="@+id/ll_title"
        android:overScrollMode="never"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="@drawable/solid_video" >

                <TextView
                    android:id="@+id/tv_equipmentname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:padding="10dp"
                    android:text="@string/left_gongju_myguardian_notcontact"
                    android:textColor="@color/title"
                    android:textSize="@dimen/size14" />

                <TextView
                    android:id="@+id/tv_tocontact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:drawableRight="@drawable/right_s"
                    android:gravity="center"
                    android:padding="10dp"
                    android:text="@string/left_gongju_myguardian_tocontact"
                    android:textColor="@color/content"
                    android:textSize="@dimen/size14" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/ll_hardware"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_time"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="时间"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_ic"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="IC卡状态"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_lystate"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="蓝牙"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_yjstate"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="硬件状态"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_dtstate"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="电池状态"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_rzstate"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="入站状态"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_battery"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="电池电量"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/my_power"
                        android:padding="10dp" />

                    <TextView
                        android:id="@+id/tv_time6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="40dp"
                        android:gravity="center"
                        android:padding="10dp "
                        android:text="功率"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />

                    <TextView
                        android:id="@+id/tv_status6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="未知"
                        android:textColor="@color/content"
                        android:textSize="@dimen/size14" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/videoview" />
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/rl_sos_location"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >
                  <TextView
                    android:id="@+id/tv_guanlian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_location"
                    android:layout_alignRight="@+id/tv_location"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/solid_sos"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp"
                    android:text="关联活动开启定位和报警功能"
                    android:textColor="@color/white"
                    android:textSize="@dimen/size24" />

                <TextView
                    android:layout_below="@+id/tv_guanlian"
                    android:id="@+id/tv_sos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_location"
                    android:layout_alignRight="@+id/tv_location"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/solid_sos"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp"
                    android:text="@string/left_gongju_myguardian_notcontact_sos"
                    android:textColor="@color/white"
                    android:textSize="@dimen/size24" />

                <TextView
                    android:id="@+id/tv_location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_sos"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/solid_location"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:paddingTop="5dp"
                    android:text="上传当前位置"
                    android:textColor="@color/white"
                    android:textSize="@dimen/size24" />
            </RelativeLayout>

            <TextView
                android:id="@+id/tv_tip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="20dp"
                android:text="@string/left_gongju_myguardian_notcontact_title"
                android:textColor="@color/red"
                android:textSize="@dimen/size24" />
        </LinearLayout>
    </com.hwacreate.outdoor.view.ReboundScrollView>

    <include
        android:id="@+id/ll_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        layout="@layout/footer_otheract" />

</RelativeLayout>