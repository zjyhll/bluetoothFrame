<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/allbackground"
    android:orientation="vertical" >

    <include layout="@layout/head_otheract" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="5dp"
        android:background="@color/videoview" />

    <com.hwacreate.outdoor.view.CleareditTextView
        android:id="@+id/yijianphone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="left|top"
        android:hint="请输入手机号码"
        android:inputType="number"
        android:lines="1"
        android:maxLength="11"
        android:padding="10dp"
        android:textColor="@color/title"
        android:textSize="@dimen/size16" >
    </com.hwacreate.outdoor.view.CleareditTextView>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/videoview" />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="8dp"
        android:background="@color/videoview" />

    <EditText
        android:id="@+id/yijianfankui"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="left|top"
        android:hint="期待您的反馈!(内容不少于5个字)"
        android:lines="6"
        android:padding="10dp"
        android:textColor="@color/title"
        android:textSize="@dimen/size16" >
    </EditText>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginBottom="20dp"
        android:background="@color/videoview" />

    <TextView
        android:id="@+id/yijianfabiao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="@drawable/select_vip_goumai"
        android:gravity="center"
        android:padding="10dp"
        android:text="发表意见"
        android:textColor="@color/white"
        android:textSize="@dimen/size16" />

</LinearLayout>