<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:id="@+id/pop_dingdan_image"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_margin="2dp"
        android:background="@drawable/solid_video_no"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/dingdan_all"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/select_item_down"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:background="@drawable/huodong_all" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="4dp"
                android:text="全部"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.5dp"
            android:background="@color/videoview" />

        <LinearLayout
            android:id="@+id/dingdan1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/select_item_down"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:background="@drawable/huodong_baomingzhong" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="4dp"
                android:text="报名中"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.5dp"
            android:background="@color/videoview" />

        <LinearLayout
            android:id="@+id/dingdan2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/select_item_down"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:background="@drawable/huodong_zhengduizhong" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="4dp"
                android:text="整队中"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.5dp"
            android:background="@color/videoview" />

        <LinearLayout
            android:id="@+id/dingdan3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/select_item_down"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:background="@drawable/huodong_zhunbei" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="4dp"
                android:text="准备出行"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.5dp"
            android:background="@color/videoview" />

        <LinearLayout
            android:id="@+id/dingdan4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/select_item_down"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:background="@drawable/huodong_chuxingzhong" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="4dp"
                android:text="出行中"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.5dp"
            android:background="@color/videoview" />

        <LinearLayout
            android:id="@+id/dingdan5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/select_item_down"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:background="@drawable/huodong_shoudui" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="4dp"
                android:text="收队中"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />
        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.5dp"
            android:background="@color/videoview" />

        <LinearLayout
            android:id="@+id/dingdan6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/select_item_down"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="6dp" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:background="@drawable/huodong_jieshu" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="4dp"
                android:text="活动结束"
                android:textColor="@color/content"
                android:textSize="@dimen/size15" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>